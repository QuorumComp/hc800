{
    "editor.insertSpaces": false,
    "files.associations": {
        "*.i": "asm.rc8",
        "*.rc8": "asm.rc8",
        "*.asm": "asm.rc8"
    },
    "[asm.rc8]": {
        "editor.tabSize": 8,
        "editor.insertSpaces": false,
        "editor.quickSuggestions": {
            "comments": "off",
            "strings": "off",
            "other": "off"
        }
    },
    "files.exclude": {
        "**/*.obj": true
    },
    "files.watcherExclude": {
        "**/target": true
    },
}